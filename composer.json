{
    "name": "yoelpc4/laravel-exchange-rate",
    "description": "Laravel Exchange Rate helper.",
    "keywords": [
        "laravel",
        "exchange rate",
        "free currency converter api"
    ],
    "homepage": "https://github.com/yoelpc4/laravel-exchange-rate",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoel Peter Chandra",
            "email": "yoelpc4@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "6.*",
        "illuminate/contracts": "6.*",
        "illuminate/support": "6.*",
        "illuminate/validation": "6.*",
        "psr/http-message": "1.*"
    },
    "require-dev": {
        "orchestra/testbench": "4.*",
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "Yoelpc4\\LaravelExchangeRate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yoelpc4\\LaravelExchangeRate\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yoelpc4\\LaravelExchangeRate\\Providers\\ExchangeRateServiceProvider"
            ],
            "aliases": {
                "ExchangeRateService": "Yoelpc4\\LaravelExchangeRate\\Facades\\ExchangeRateService"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
